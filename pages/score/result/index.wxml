<view class="swiper-tab">
 <view class="swiper-tab-list {{currentTab==0 ? 'on' : ''}}" data-current="0" bindtap="swichNav">考核中</view>
 <view class="swiper-tab-list {{currentTab==1 ? 'on' : ''}}" data-current="1" bindtap="swichNav">合格成绩</view>
 <!-- <view class="swiper-tab-list {{currentTab==2 ? 'on' : ''}}" data-current="2" bindtap="swichNav">Seside3</view> -->
</view>
<swiper current="{{currentTab}}" class="swiper-box" duration="300" style="height:{{winHeight - 31}}px" bindchange="bindChange">
 <swiper-item>
  <!-- <view></view> -->
  <view wx:for="{{exam_ing}}">
  {{item.bm_name}} {{item.bm_zy}} {{item.bm_level}} {{item.bm_time}}
</view>
 </swiper-item>

 <swiper-item>
  <!-- <view></view> -->
    <view wx:for="{{exam_done}}">
  {{item.bm_name}} {{item.bm_zy}} {{item.pass_level}} {{item.bm_time}}
</view>
  <!-- <view data-text="{{item.text}}" class="static_cell" bindtap="{{item.func}}">{{item.text}}</view> -->

 </swiper-item>
</swiper>