.swiper-tab{
  width: 100%; 
  border-bottom: 2rpx solid #777777; 
  margin-bottom: 10px;
  text-align: center; 
  line-height: 80rpx;
}
.swiper-tab-list{
  font-size: 14pt; 
  font-weight: 600;
  display: inline-block; 
  width: 40%; 
  color: #777777; 
}
.on{
  color: #09bb07; 
  border-bottom: 5rpx solid #da7c0c;
}
.swiper-box{ 
  display: block;
  height: 100%;
  width: 100%;
  overflow: hidden;
 }
.swiper-box view{ 
  text-align: center; 
}
.line-css{
  height: 2rpx;
  background: #777;
  margin: 20rpx 0 20rpx 0;
}

.detail{
  font-size: 11pt;
  font-weight: 600;
  font-stretch: ultra-expanded;
  color: #353535;
  display: flex;  
  flex-direction: column;
  width: 100%;
  height: 100%;
  flex:1;
  justify-content: left;
  margin: 30rpx 20rpx 0px 20rpx;
}
.row{
  display: flex;  
  flex-direction: row;
  width: 100%;
  height: 100%;
  flex:1;
  justify-content: left;
  margin:0px 20rpx 0px 40rpx;
}
.row text{
  width: 200rpx;
  text-align: left;
  margin:0px 0 20rpx 0;
}
.row input{
  border: 2rpx solid #888888;
  background-color: #f6f6f6;
  width: 60%;
  height: 72rpx;
  line-height: 72rpx;
  text-align: left;
}
.undis{
  display: none;
}
.dis{
  display: block;
}
.ilogo{
  display: flex; 
  flex-wrap: nowrap; 
  flex-direction:row;
  width: 100%;
  min-height: 72px;
  justify-content: center;
  align-items: center;
}
.list-item{
  font-size: 11pt;
  font-weight: 600;
  font-stretch: ultra-expanded;
  color: #353535;
  display: flex;  
  flex-direction: row;
  width: 100%;
  min-height: 72rpx;
  justify-content: left;
  align-items: center;
}

.list-item text{
  width: 100%;
  height: 72rpx;
  line-height: 72rpx;
  text-align: center;
}
.border-css{
  height: 2rpx;
  min-height: 2rpx;
  background: #ccc;
  margin: 20rpx 0 20rpx 0;
}